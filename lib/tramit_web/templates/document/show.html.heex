<h1>Show Document</h1>

<ul>

  <li>
    <strong>Name:</strong>
    <%= @document.name %>
  </li>

  <li>
    <strong>Title:</strong>
    <%= @document.title %>
  </li>

  <li>
    <strong>Content:</strong>
    <%= @document.content %>
  </li>

</ul>

<span><%= link "Edit", to: Routes.document_path(@conn, :edit, @document) %></span> |
<span><%= link "Back", to: Routes.document_path(@conn, :index) %></span>
